                                            a#formsSolidPugTab.nav-link(data-toggle='tab', href='#formsSolidPug', role='tab', aria-controls='formsSolidPug', aria-selected='false')
                                                img.img-pug.mr-1(src='assets/img/pug.svg')
                                                | PUG
                                    // Tab panes
                                    .tab-content
                                        #formsSolidHtml.tab-pane.active(role='tabpanel', aria-labelledby='formsSolidHtmlTab')
                                            pre.language-markup
                                                code.
                                                    <script type="text/plain"><form>
                                                        <div class="form-group"><label for="exampleFormControlInput1">Email address</label><input class="form-control form-control-solid" id="exampleFormControlInput1" type="email" placeholder="name@example.com"></div>
                                                        <div class="form-group">
                                                            <label for="exampleFormControlSelect1">Example select</label><select class="form-control form-control-solid" id="exampleFormControlSelect1">
                                                                <option>1</option>
                                                                <option>2</option>
